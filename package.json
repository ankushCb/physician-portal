{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --ext .jsx --ext .js --fix",
    "watch": "webpack -d --watch",
    "build-dev": "cross-env NODE_ENV=production BACKEND='dev' webpack --config webpack.prod.js",
    "build-staging": "cross-env NODE_ENV=production BACKEND='staging' webpack --config webpack.prod.js",
    "build-prod": "cross-env NODE_ENV=production BACKEND='prod' webpack --config webpack.prod.js",
    "build-deploy-test": "env-cmd ../env/deploy_test npm run build",
    "start": "cross-env NODE_ENV=local webpack-dev-server --hot --inline --content-base ./src/client --port $PHYSICIAN_APP_PORT --host 0.0.0.0",
    "start-dev": "env-cmd ../env/dev npm start",
    "start:prod-server": "cross-env NODE_ENV=production node server --port 8080",
    "start:production": "npm run build && npm run start:prod-server",
    "test": "NODE_PATH=./src/client/app mocha src/client/app/scripts/modules/**/test/**.js --compilers js:babel-core/register --require babel-polyfill --timeout 0",
    "integration-test": "node nightwatch -e chrome",
    "toolbox": "react-toolbox-themr"
  },
